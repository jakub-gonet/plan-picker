<ul>
<%= if @current_password_auth do %>
  <li><%= @current_password_auth.email %></li>
  <li><%= link "Settings", to: Routes.password_auth_settings_path(@conn, :edit) %></li>
  <li><%= link "Log out", to: Routes.password_auth_session_path(@conn, :delete), method: :delete %></li>
<% else %>
  <li><%= link "Register", to: Routes.password_auth_registration_path(@conn, :new) %></li>
  <li><%= link "Log in", to: Routes.password_auth_session_path(@conn, :new) %></li>
<% end %>
</ul>
